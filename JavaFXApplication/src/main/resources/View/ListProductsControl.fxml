<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<FlowPane prefHeight="500.0" prefWidth="596.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="javafxapplication.Controller.ProductsInShop.ListProductsController">
  <children>
    <Button mnemonicParsing="false" onAction="#textTolukta" prefHeight="27.0" prefWidth="88.0" text="Толукта">
      <FlowPane.margin>
        <Insets bottom="15.0" />
      </FlowPane.margin>
    </Button>
    <Label prefHeight="27.0" prefWidth="261.0" text="Дукондогу буйумдардын тизмеси">
      <font>
        <Font size="16.0" />
      </font>
      <FlowPane.margin>
        <Insets left="60.0" />
      </FlowPane.margin>
    </Label>
    <TableView fx:id="tableView2" prefHeight="200.0" prefWidth="596.0">
      <columns>
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="127.0" text="Буйумдун аты" fx:id="productName" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="106.0" text="Буйумдун баасы" fx:id="productPrice" />
        <TableColumn prefWidth="110.0" text="Буйумдун саны" fx:id="productCount" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="117.0" text="Дукондун аты" fx:id="shopName" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="133.0" text="Жабдуучунун аты" fx:id="providerName" />
      </columns>
      <FlowPane.margin>
        <Insets right="15.0" />
      </FlowPane.margin>
    </TableView>
  </children>
</FlowPane>
